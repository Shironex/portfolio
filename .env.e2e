# E2E Test Environment Variables
# These are minimal variables needed for E2E tests

# Node Environment
NODE_ENV=test
CI=true

# Public URL (required)
NEXT_PUBLIC_PUBLIC_URL=http://localhost:3000

# Cloudflare Turnstile (required for client)
NEXT_PUBLIC_TURNSTILE_SITE_KEY=test_turnstile_site_key

# Server-side variables (can be empty in E2E tests)
REDIS_HOST=redis://localhost:6379
RESEND_API_KEY=test_key
RESEND_MAIL_TO=test@example.com
TURNSTILE_SECRET_KEY=test_secret
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/test/test
ANALYTIC_URL=https://analytics.example.com
ANALYTIC_ID=test_id

# Sentry (disabled)
SENTRY_DSN=
